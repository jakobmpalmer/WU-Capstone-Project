<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="290dp"
        android:layout_height="205dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.156"
        app:srcCompat="@drawable/logo_transp" />

    <Button
        android:id="@+id/logging_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logging"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="493dp" />

    <Button
        android:id="@+id/analitics_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:text="Analytics"
        app:layout_constraintBottom_toBottomOf="@+id/logging_btn"
        app:layout_constraintStart_toEndOf="@+id/logging_btn"
        app:layout_constraintTop_toTopOf="@+id/logging_btn"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/settings_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/action_settings"
        app:layout_constraintEnd_toEndOf="@+id/logging_btn"
        app:layout_constraintStart_toStartOf="@+id/logging_btn"
        tools:layout_editor_absoluteY="618dp" />
</androidx.constraintlayout.widget.ConstraintLayout>